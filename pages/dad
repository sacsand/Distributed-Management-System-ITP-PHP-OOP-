        $(document).ready(function (){
            $("#maincategory").change(function() {
                // foo is the id of the other select box
                if ($(this).val() == "Jumbo Peanut") {
                    $("#foo").show();
                }else{
                    $("#foo").hide();
                }

                if ($(this).val() == "Wafers") {
                    $("#foo1").show();
                }else{
                    $("#foo1").hide();
                }

